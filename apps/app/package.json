{
  "name": "@fridge/app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "-----QUICK MENU-----": "",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "app:start": "npm-run-all --parallel app:tailwind:dev app:start:dev",
    "app:android": "react-native run-android",
    "app:ios": "react-native run-ios",
    "-----DEV MENU-----": "",
    "app:start:dev": "react-native start",
    "app:tailwind:dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "-----BUILD MENU-----": "",
    "app:tailwind:build": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn"
  },
  "dependencies": {
    "@fridge/fridge": "*",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@reduxjs/toolkit": "^1.8.0",
    "clsx": "^1.1.1",
    "i18next": "^21.6.12",
    "react": "17.0.2",
    "react-native": "^0.67.3",
    "react-native-safe-area-context": "^4.0.1",
    "react-native-screens": "^3.12.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.0.0",
    "eslint": "^7.14.0",
    "get-yarn-workspaces": "^1.0.2",
    "jest": "^26.6.3",
    "metro-config": "^0.69.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "react-test-renderer": "17.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
